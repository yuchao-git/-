.state-show .tab-box {
  height: 80rpx;
  display: flex;
  justify-content: space-between;
}
.state-show .tab-box .tab-item {
  width: 33.33%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1px solid #999;
}
.state-show .tab-box .tab-item.active {
  background-color: #15f1f1;
  color: white;
}
.state-show .map-item {
  margin-top: 20rpx;
}
.state-show .refresh {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
